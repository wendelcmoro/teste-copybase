{
    "version": 2,
    "builds": [
      { "src": "nuxt.config.js", "use": "@nuxtjs/vercel-builder" }
    ],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/(.*).(js|css|svg|png)",
        "dest": "/$1.$2",
        "headers": { "cache-control": "public, max-age=31536000, immutable" }
      },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  